<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>simulator_2d</name>
  <version>0.0.1</version>
  <description>The 2D physics simulator based on Ackerman kinematics.</description>
  <maintainer email="olek.olegovich@gmail.com">root</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>model</depend>
  <depend>nav_msgs</depend>
  <depend>truck_msgs</depend>
  <depend>geom</depend>
  <depend>tf2_msgs</depend>
  <depend>control_proxy</depend>
  <depend>visualization</depend>
  <depend>visualization_msgs</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>